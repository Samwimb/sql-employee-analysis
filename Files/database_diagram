dept
-
dept_no varchar pk
dept_name varchar

dept_emp
-
emp_no varchar fk - emp.emp_no
dept_no varchar fk >-< dept.dept_no
from_date varchar
to_date varchar

dept_manager
-
dept_no varchar fk -< dept.dept_no
emp_no varchar fk - emp.emp_no
from_date varchar
to_date varchar

emp
-
emp_no varchar pk
birth_date varchar
first_name varchar
last_name varchar
gender varchar
hire_date varchar

salaries
-
emp_no varchar fk - emp.emp_no
salary int
from_date varchar
to_date varchar

titles
-
emp_no varchar fk - emp.emp_no
title varchar
from_date varchar
to_date varchar
